{
  "name": "bravesheep/flysystem-url-bundle",
  "description": "FlySystem URL bundle generates services for flysystem by specifying a url parameter",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Bravesheep\\FlysystemUrlBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bravesheep\\FlysystemUrlBundle\\Test\\": "test/"
    }
  },
  "authors": [
    {
      "name": "Ruben Nijveld",
      "email": "ruben@gewooniets.nl"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "symfony/config": "^2.3|^3.0",
    "symfony/dependency-injection": "^2.3|^3.0",
    "symfony/http-kernel": "^2.3|^3.0"
  },
  "require-dev": {
    "phpunit/phpunit" : "^5.3"
  },
  "suggest": {
    "oneup/flysystem-bundle": "^1.3.0",
    "league/flysystem": "^1.0.0"
  }
}
